{
    "name": "devon",
    "version": "1.0.0",
    "description": "Console bot to assist with common development tasks.",
    "main": "index.js",
    "scripts": {
        "start": "tsc && node ./lib/index.js",
        "test": "jest"
    },
    "jest": {
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "json"
        ],
        "moduleNameMapper": {
            "applicationinsights-js": "identity-obj-proxy"
        },
        "transform": {
            "^.+\\.(ts|tsx)$": "ts-jest/preprocessor.js"
        },
        "testMatch": [
            "**/*.spec.(ts|tsx)"
        ],
        "collectCoverage": true,
        "coverageDirectory": "./coverage"
    },
    "author": "stevenic",
    "license": "MIT",
    "dependencies": {
        "@types/command-line-args": "^5.0.0",
        "@types/node": "^10.12.12",
        "@types/string-argv": "^0.1.0",
        "@types/uuid": "^3.4.4",
        "botbuilder": "^4.1.7",
        "botbuilder-dialogs": "^4.1.7",
        "chalk": "^2.4.1",
        "command-line-args": "^5.0.2",
        "node-uuid": "^1.4.8",
        "string-argv": "^0.1.1",
        "underscore": "^1.9.1",
        "uuid": "^3.3.2"
    },
    "devDependencies": {
        "@types/jasmine": "^3.3.5",
        "@types/jest": "^23.3.11",
        "jasmine": "^3.3.1",
        "jest": "^23.6.0",
        "ts-jest": "^23.10.5",
        "typescript": "^3.2.1"
    }
}
